{
  "name":"HelloWorld",
  "pomFile": {"groupId":"br.com.tarek", "artifactId":"HelloWorld", "description":"Testing the Generator", "javaVersion":"1.7"},
  "beans":[
    {"name":"Hello", "scope":"SESSION", "fields":[{"type":"String", "name":"name"}]}
  ],
  "views":[
    {"name":"hello", "welcomeFile":true, "content": {"name":"html","properties":{"xmlns":"http://www.w3.org/1999/xhtml","xmlns:h":"http://java.sun.com/jsf/html"},
      "contents":[
        {"name":"h:head","contents":[{"name":"title","value":"Hello World"}]},
        {"name":"h:body","contents":[{"name":"h:form","contents":[
          {"name":"h:inputText","properties":{"value":"#{helloBean.name}"}},
          {"name":"h:commandButton","properties":{"value":"ok","action":"hello-result"}}
        ]}
      ]}]
    }},
    {"name":"hello-result", "content": {"name":"html","properties":{"xmlns":"http://www.w3.org/1999/xhtml","xmlns:h":"http://java.sun.com/jsf/html"},
      "contents":[
        {"name":"h:head","contents":[{"name":"title","value":"Hello World"}]},
        {"name":"h:body","contents":[{"name":"h4","value":"Hello, #{helloBean.name}"}]}
      ]
    }}
  ]
}